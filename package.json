{
  "name": "uswds-sandbox",
  "private": true,
  "author": "18F",
  "license": "CC0-1.0",
  "scripts": {
    "federalist": "npm run uswds-copy-assets && npm run uswds-build-sass",
    "jekyll-install": "gem install bundler && bundle install",
    "serve": "bundle exec jekyll serve --livereload",
    "start": "npm install && npm update uswds && npm run jekyll-install && npm run uswds-copy-assets && npm run uswds-copy-theme && npm run uswds-build-sass",
    "uswds-build-sass": "gulp build-sass",
    "uswds-build-app": "gulp build-app",
    "uswds-copy-assets": "npm run uswds-copy-js && npm run uswds-copy-fonts && npm run uswds-copy-img",
    "uswds-copy-fonts": "gulp copy-uswds-fonts",
    "uswds-copy-img": "gulp copy-uswds-images",
    "uswds-copy-js": "gulp copy-uswds-js",
    "uswds-copy-theme": "gulp copy-uswds-setup",
    "uswds-update": "npm update uswds",
    "watch": "gulp watch",
    "snyk-protect": "snyk protect",
    "prepare": "npm run snyk-protect",
    "startup": "bundle exec jekyll serve --detach",
    "pa11y-ci:sitemap": "pa11y-ci --sitemap http://localhost:4000/sitemap.xml --sitemap-exclude '/*.pdf|next/'",
    "pa11y-ci:sitemap-mobile": "pa11y-ci --config .pa11yci--mobile --sitemap http://localhost:4000/sitemap.xml --sitemap-exclude '/*.pdf|next/'",
    "pa11y-ci:sitemap-json": "pa11y-ci --json > pa11y-results.json --sitemap http://localhost:4000/sitemap.xml --sitemap-exclude '/*.pdf|next/'"
  },
  "dependencies": {
    "acorn": "^8.7.0",
    "autoprefixer": "^10.4.2",
    "braces": "^3.0.2",
    "css-mqpacker": "^7.0.0",
    "cssnano": "^4.1.4",
    "del": "^6.0.0",
    "gulp": "^4.0.2",
    "gulp-gzip": "^1.4.2",
    "gulp-postcss": "^9.0.1",
    "gulp-rename": "^2.0.0",
    "gulp-replace": "^1.1.3",
    "gulp-sass": "^4.1.0",
    "gulp-size": "^3.0.0",
    "gulp-sourcemaps": "^3.0.0",
    "gulp-watch": "^5.0.0",
    "path": "^0.12.7",
    "postcss": "8.4.5",
    "postcss-csso": "^5.0.1",
    "postcss-uncss": "^0.17.0",
    "sass": "^1.48.0",
    "snyk": "^1.833.0",
    "uncss": "^0.17.3",
    "uswds": "^2.12.1",
    "uswds-gulp": "github:uswds/uswds-gulp"
  },
  "snyk": true,
  "devDependencies": {
    "pa11y-ci": "^3.0.1"
  }
}
